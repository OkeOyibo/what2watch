{
  "expo": {
    "name": "what2watch",
    "slug": "what2watch",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "app/assets/icon.png",
    "description": "A simple cross-platform application, which gives you the movie / series recommandations you want!",
    "splash": {
      "image": "app/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "app/assets/icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "app/assets/favicon.png"
    }
  }
}
